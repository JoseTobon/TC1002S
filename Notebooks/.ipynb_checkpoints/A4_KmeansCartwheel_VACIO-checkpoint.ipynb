   "source": [
    "# Kmeans clustering\n",
    "\n",
    "Kmeans clustering"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 220,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "array([1, 1, 2, 2, 3, 3, 0, 2, 0, 1, 2, 1, 2, 1, 3, 0, 2, 1, 3, 2, 3, 0,\n",
       "       2, 3, 1])"
      ]
     },
     "execution_count": 220,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "from sklearn.cluster import KMeans\n",
    "\n",
    "\n",
    "K  = 4\n",
    "\n",
    "# Crear objeto para hacer kmeans y definir el numero de clusters\n",
    "km = KMeans(n_clusters=K)\n",
    "\n",
    "# Do kmeans\n",
    "clusters = km.fit_predict(dataset[['Wingspan','CWDistance','Height']])\n",
    "\n",
    "clusters"
   ]
